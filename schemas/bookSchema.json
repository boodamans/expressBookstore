{
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "isbn",
        "amazon_url",
        "author",
        "language",
        "pages",
        "publisher",
        "title",
        "year"
    ],
    "properties": {
        "isbn": {
            "type": "string",
            "default": "",
            "title": "The isbn Schema",
            "examples": [
                "9781234567890"
            ]
        },
        "amazon_url": {
            "type": "string",
            "default": "",
            "title": "The amazon_url Schema",
            "examples": [
                "https://www.amazon.com/example-book"
            ]
        },
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "John Doe"
            ]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "English"
            ]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                300
            ]
        },
        "publisher": {
            "type": "string",
            "default": "",
            "title": "The publisher Schema",
            "examples": [
                "ABC Publications"
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "The Secret of the Lost Key"
            ]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                2022
            ]
        }
    },
    "examples": [{
        "isbn": "9781234567890",
        "amazon_url": "https://www.amazon.com/example-book",
        "author": "John Doe",
        "language": "English",
        "pages": 300,
        "publisher": "ABC Publications",
        "title": "The Secret of the Lost Key",
        "year": 2022
    }]
}